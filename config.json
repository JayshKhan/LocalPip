{
    "network": {
        "pypi_mirror": "https://pypi.org/simple/",
        "timeout": 30,
        "max_concurrent": 5,
        "verify_ssl": true
    },
    "ui": {
        "theme": "Light",
        "window_size": {
            "width": 1512,
            "height": 831
        }
    },
    "download": {
        "default_path": "/home/internet/downloaded_wheels",
        "include_dependencies": true,
        "verify_checksums": true,
        "python_version": "3.11",
        "platform": "any"
    }
}